+#: src/login.c:124
+msgid "Usage: login [name] [-p] [-h host] [-f name]\n"
+msgstr "Kullanım: login [isim] [-p] [-h makina] [-r isim]\n"
+
+#: src/login.c:132
+msgid ""
+"login - sign on\n"
+"\n"
+msgstr "login - sisteme giriş yap\n"
+
+#: src/login.c:134
+msgid "  -p             Do not destroy the environment\n"
+msgstr "  -p             Çevresel değişkenleri yok etme!\n"
+
+#: src/login.c:135
+msgid "  -h hostname    Name of the remote host for umtp/wtmp\n"
+msgstr "  -h makina      umtp/wtmp için uzak makina adı.\n"
+
+#: src/login.c:137
+msgid "  -f name        Skip a second login authentication\n"
+msgstr ""
+"  -f isim        login için ikinci kullanıcı doğrulama işlemini atla.\n"
+
+#: src/login.c:138
+msgid "  --help         Give this help list\n"
+msgstr "  --help         Bu yardım ekranını göster.\n"
+
+#: src/login.c:139
+msgid "  --usage        Give a short usage message\n"
+msgstr "  --usage        Kısa bir kullanım mesajı göster.\n"
+
+#: src/login.c:140
+msgid "  --version      Print program version\n"
+msgstr "  --version      Program Sürüm numarasını göster.\n"
+
+#: src/login.c:149
+#, c-format
+msgid "Try `%s --help' or `%s --usage' for more information.\n"
+msgstr ""
+"Daha fazla bilgi için `%s --help' ya da `%s --usage' kumutlarını "
+"kullanabilirsiniz.\n"
+
+#: src/login.c:206
+#, c-format
+msgid "Login timed out after %d seconds\n"
+msgstr "%d saniye sonra giriş için izin verilen süre doldu\n"
+
+#: src/login.c:224
+#, c-format
+msgid "Warning!  Could not get current context for %s, not relabeling.\n"
+msgstr ""
+
+#: src/login.c:231
+#, c-format
+msgid "Warning!  Could not get new context for %s, not relabeling.\n"
+msgstr ""
+
+#: src/login.c:237 src/login.c:253
+#, c-format
+msgid "Warning!  Could not relabel %s with %s, not relabeling.\n"
+msgstr ""
+
+#: src/login.c:393
+#, c-format
+msgid "login: lseek() failed: %s\n"
+msgstr "login: lseek() hatası: %s\n"
+
+#: src/login.c:404
+#, c-format
+msgid "Last login: %.*s "
+msgstr "Son giriş: %.*s "
+
+#: src/login.c:408
+#, c-format
+msgid "from %.*s\n"
+msgstr "%.*s makinası.\n"
+
+#: src/login.c:411
+#, c-format
+msgid "on %.*s\n"
+msgstr "%.*s sanal konsol.\n"
+
+#: src/login.c:483
+msgid " login: "
+msgstr " kullanıcı adı: "
+
+#: src/login.c:488
+msgid "login:"
+msgstr "kullanıcı adı: "
+
+#: src/login.c:550
+#, c-format
+msgid "login: gethostname() failed: %s\n"
+msgstr "login: gethostname() hatası: %s\n"
+
+#: src/login.c:591
+msgid "login: -h for super-user only.\n"
+msgstr "login: -h sadece sisten yöneticisi tarafından kullanılabilir.\n"
+
+#: src/login.c:597 src/login.c:609 src/login.c:628 src/login.c:674
+msgid "login: out of memory.\n"
+msgstr "login: bellek yetersizliği.\n"
+
+#: src/login.c:663
+msgid "login: Too many arguments\n"
+msgstr "login: Gereğinden fazla parametre\n"
+
+#: src/login.c:711
+#, c-format
+msgid "FATAL: cannot change permissions of TTY: %s"
+msgstr "HATA: sanal konsol'un hakları değiştirilemiyor, TTY: %s"
+
+#: src/login.c:747
+#, c-format
+msgid "login: PAM Failure, aborting: %s\n"
+msgstr "login: PAM Hatası, program sonlanıyor: %s\n"
+
+#: src/login.c:826
+msgid ""
+"Login incorrect\n"
+"\n"
+msgstr ""
+"Giriş hatalı\n"
+"\n"
+
+#: src/login.c:859 src/login.c:904 src/login.c:912
+msgid ""
+"\n"
+"Session setup problem, abort.\n"
+msgstr ""
+"\n"
+"Oturum kurulumu hatası, program sonlanıyor.\n"
+
+#: src/login.c:883
+#, c-format
+msgid "exceeded failure limit for `%s'%s\n"
+msgstr "`%s'%s için hata limiti aşıldı\n"
+
+#: src/login.c:937
+#, c-format
+msgid "%s:  query_user_context failed\n"
+msgstr ""
+
+#: src/login.c:945
+#, c-format
+msgid "login: unable to obtain context for %s.\n"
+msgstr ""
+
+#: src/login.c:969
+msgid ""
+"Too many users logged on already.\n"
+"Try again later.\n"
+msgstr ""
+"Şimdiden çok fazla kullanıcı sisteme bağlı.\n"
+"Daha sonra tekrar deneyin.\n"
+
+#: src/login.c:973
+msgid "You have too many processes running.\n"
+msgstr "Çok fazla çalışan süreç var.\n"
+
+#: src/login.c:1182
+msgid "login: setenv (\"HOME\") failed: insufficient space."
+msgstr "login: setenv (\"HOME\") hatası: yetersiz alan."
+
+#: src/login.c:1190
+msgid "login: setenv (\"SHELL\") failed: insufficient space."
+msgstr "login: setenv (\"SHELL\") hatası: yetersiz alan."
+
+#: src/login.c:1193
+msgid "login: setenv (\"TERM\") failed: insufficient space."
+msgstr "login: setenv (\"TERM\") hatası: yetersiz alan."
+
+#: src/login.c:1204
+msgid "login: setenv (\"MAIL\") failed: insufficient space."
+msgstr "login: setenv (\"MAIL\") hatası: yetersiz alan."
+
+#: src/login.c:1211
+msgid "login: setenv (\"USER\") failed: insufficient space."
+msgstr "login: setenv (\"USER\") hatası: yetersiz alan."
+
+#: src/login.c:1214
+msgid "login: setenv (\"LOGNAME\") failed: insufficient space."
+msgstr "login: setenv (\"LOGNAME\") hatası: yetersiz alan."
+
+#: src/login.c:1284
+#, c-format
+msgid "login: failure forking: %s"
+msgstr "login: fork sırasında hata: %s"
+
+#: src/login.c:1303
+#, c-format
+msgid "login: waitpid (%d, NULL, 0) failed: %s\n"
+msgstr "login: waitpid (%d, NULL, 0) hatası: %s\n"
+
+#: src/login.c:1329
+#, c-format
+msgid "login: couldn't set controlling tty: %s\n"
+msgstr "login: kontrol edilebilir sanal konsol oluşturulamadı, tty: %s\n"
+
+#: src/login.c:1344
+#, c-format
+msgid "Error!  Unable to set executable context %s.\n"
+msgstr "Hata!  Çalıştırılabilir ortam oluşturulamadı %s.\n"
+
+#: src/login.c:1364
+#, c-format
+msgid "No directory %s!\n"
+msgstr "%s dizini yok!\n"
+
+#: src/login.c:1370
+msgid "Logging in with home = \"/\".\n"
+msgstr "Ev dizini olarak \"/\" atanıyor.\n"
+
+#: src/login.c:1380
+msgid "login: no memory for shell script.\n"
+msgstr "login: kabuk betiği için yeterli bellek yok.\n"
+
+#: src/login.c:1412
+#, c-format
+msgid "login: couldn't exec shell script: %s.\n"
+msgstr "login: kabuk betiği çalıştırılamıyor: %s.\n"
+
+#: src/login.c:1415
